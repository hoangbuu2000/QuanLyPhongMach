<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<div class="page-wrapper">
    <div class="content">
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <c:if test="${disease.tenBenh == null}">
                    <h4 class="page-title"><spring:message code="disease.add.page.title" /></h4>
                </c:if>
                <c:if test="${disease.tenBenh != null}">
                    <h4 class="page-title"><spring:message code="disease.edit.page.title" /></h4>
                </c:if>
            </div>
        </div>
        <form:form modelAttribute="disease"  method="post">
            <form:errors path="*" cssClass="text-danger" />
            <p class="text-info">${message}</p>
            <div class="row">
                <div class="col-lg-8 offset-lg-2">
                    <form>
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-12 col-md-12 col-lg-6">
                                        <div class="form-group">
                                            <c:if test="${disease.tenBenh == null}">
                                                <label><spring:message code="disease.add.label.name" /></label>
                                                <form:input path="tenBenh" cssClass="form-control" />
                                            </c:if>
                                            <c:if test="${disease.tenBenh != null}">
                                                <label><spring:message code="disease.add.label.name" /></label>
                                                <form:input path="tenBenh" cssClass="form-control" value="${disease.tenBenh}" />
                                            </c:if>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <c:if test="${disease.tenBenh == null}">
                                        <label><spring:message code="disease.add.label.des" /></label>
                                        <form:input path="moTa" cssClass="form-control" />
                                    </c:if>
                                    <c:if test="${disease.tenBenh != null}">
                                        <label><spring:message code="disease.add.label.des" /></label>
                                        <form:input path="moTa" cssClass="form-control" value="${disease.moTa}" />
                                    </c:if>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-12 col-md-12 col-lg-6">
                                        <div class="form-group">
                                            <c:if test="${disease.tenBenh == null}">
                                                <label><spring:message code="disease.add.label.price" /></label>
                                                <form:input path="donGia" cssClass="form-control" />
                                            </c:if>
                                            <c:if test="${disease.tenBenh != null}">
                                                <label><spring:message code="disease.add.label.price" /></label>
                                                <form:input path="donGia" cssClass="form-control" value="${disease.donGia}" />
                                            </c:if>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="m-t-20 text-center">
                            <button class="btn btn-primary submit-btn">
                                <c:if test="${disease.tenBenh == null}">
                                    <spring:message code="disease.add.form.submit" />
                                </c:if>
                                <c:if test="${disease.tenBenh != null}">
                                    <spring:message code="disease.edit.form.submit" />
                                </c:if>
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </form:form>
